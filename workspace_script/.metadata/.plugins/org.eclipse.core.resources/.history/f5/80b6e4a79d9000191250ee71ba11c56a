<!-- document.write()역할,기능, 왜? -->
<script type="text/javascript">
	window.onload = function(){//익명함수
		var img = document.createElement("img");
		//속성 추가(지정) setAttribute([1],[2])
		img.setAttribute("src","../images/hu.jpg");
		//insert here
		//body안에 노드를 추가
		document.body.appendChild(img);
	}
</script>
