	public void setSqlSessionTemplate(SqlSessionTemplate sqlSessionTemplate) {
		this.sqlSessionTemplate = sqlSessionTemplate;
	}
	public void empInsert(Object object) {
		logger.info("empInsert 호출 성공");
	}
}
